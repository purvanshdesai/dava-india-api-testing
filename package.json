{
  "name": "dava-india-server",
  "description": "Dava India Server",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "mukeshtp",
    "email": "mukesh@teampumpkin.com"
  },
  "contributors": [],
  "bugs": {},
  "engines": {
    "node": ">= 20.10.0"
  },
  "feathers": {
    "language": "ts",
    "packager": "yarn",
    "database": "mongodb",
    "framework": "koa",
    "transports": [
      "rest",
      "websockets"
    ],
    "schema": "typebox"
  },
  "directories": {
    "lib": "src",
    "test": "test"
  },
  "files": [
    "lib/client.js",
    "lib/**/*.d.ts",
    "lib/**/*.shared.js"
  ],
  "main": "lib/client",
  "scripts": {
    "dev": "set NODE_ENV=development && nodemon -x ts-node src/index.ts",
    "compile": "shx rm -rf lib/ && tsc",
    "start": "node lib/",
    "prettier": "npx prettier \"**/*.ts\" --write",
    "test": "cross-env JEST_WATCHMAN=0 BABEL_ENV=test jest --maxWorkers=2 --no-watchman --no-cache",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "bundle:client": "npm run compile && npm pack --pack-destination ./public",
    "migrate": "migrate-mongo up",
    "migrate:rollback": "migrate-mongo down"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.1",
    "@feathersjs/adapter-commons": "^5.0.30",
    "@feathersjs/authentication": "^5.0.30",
    "@feathersjs/authentication-client": "^5.0.30",
    "@feathersjs/authentication-local": "^5.0.30",
    "@feathersjs/authentication-oauth": "^5.0.30",
    "@feathersjs/configuration": "^5.0.30",
    "@feathersjs/errors": "^5.0.30",
    "@feathersjs/feathers": "^5.0.30",
    "@feathersjs/koa": "^5.0.30",
    "@feathersjs/mongodb": "^5.0.30",
    "@feathersjs/schema": "^5.0.30",
    "@feathersjs/socketio": "^5.0.30",
    "@feathersjs/transport-commons": "^5.0.30",
    "@feathersjs/typebox": "^5.0.30",
    "@koa/multer": "^3.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/koa__multer": "^2.0.7",
    "@types/pdfmake": "^0.2.9",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.21.2",
    "csv-parser": "^3.2.0",
    "dayjs": "^1.11.13",
    "excel4node": "^1.8.2",
    "exceljs": "^4.4.0",
    "feathers-swagger": "^3.0.0",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "koa-mount": "^4.0.0",
    "migrate-mongo": "^11.0.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "mongodb": "^6.8.0",
    "mongoose": "^8.6.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "3.3.4",
    "nodemailer": "^6.9.15",
    "number-to-words": "^1.2.4",
    "payu-websdk": "^1.2.0",
    "pdfmake": "^0.2.15",
    "promise-limit": "^2.7.0",
    "razorpay": "^2.9.4",
    "redis-json": "^6.0.3",
    "swagger-ui-dist": "^5.18.2",
    "twilio": "^5.3.5",
    "uuid": "^11.0.4",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@feathersjs/cli": "5.0.30",
    "@feathersjs/rest-client": "^5.0.30",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.1",
    "@types/nodemailer": "^6.4.16",
    "@types/number-to-words": "^1.2.3",
    "cross-env": "^7.0.3",
    "jest": "^30.0.5",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "razorpay-typescript": "^1.0.12",
    "shx": "^0.3.4",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
